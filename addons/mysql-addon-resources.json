{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {
    "name": "mysql-driver-addon"
  },
  "items": [
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "mysql-driver-addon"
      },
      "spec": {
        "triggers": [],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/jorgemoralespou/ose-extended-builds.git",
            "ref": "master"
          },
          "contextDir": "addons/mysql"
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "centos:centos7",
              "namespace": "openshift"
            }
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "mysql-driver-addon:latest"
          }
        },
        "resources": {}
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "mysql-driver-addon"
      },
      "spec": {
        "dockerImageRepository": "",
        "tags": [
          {
            "name": "latest",
            "annotations": {
              "description": "Mysql driver addon",
              "tags": "mysql,jdbc-driver,addon",
              "version": "1.0"
            }
          }
        ]
      }
    }
  ]
}